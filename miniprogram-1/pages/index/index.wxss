/* pages/index/index.wxss */
.header .title {
  font-size: 35rpx;
  line-height: 80rpx;
  color: #666;
}
.indexContainer{
  /**
  box-shadow: inset 0 0 10rpx #777777;
  border:10rpx solid
  **/
}
.header .more {
  font-size: 40rpx;
  text-align: right;
}
.header{
  padding-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.contestsList{
  border-top: 5rpx solid #777777;
  border-bottom: 5rpx solid #777777;
  padding-top: 10rpx;
  display: flex;
  flex-direction: column;
}
image{
  width: 100%;
  height: 100%;
}
.back{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.main{
  z-index: 1;
}
.text{
  font-weight: 600;
  font-size: 30rpx;
  margin-left:10rpx;
}